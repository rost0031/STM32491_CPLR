me-&gt;msgPayloadName = _DC3StatusPayloadMsg;
me-&gt;basicMsg._msgPayload = me-&gt;msgPayloadName;

me-&gt;errorCode = ERR_MSG_UNEXPECTED_PAYLOAD;
